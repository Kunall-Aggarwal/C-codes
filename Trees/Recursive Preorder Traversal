class TreeNode{
	int val;
	TreeNode left;
	TreeNode right;
	TreeNode(){}
	TreeNode(int v) { this.val = v; }
	TreeNode(int v){
		this.val = v;
		this.left = left;
		this.right = right;
	}
}

class Solution{
	public void preorderTraversal(TreeNode root){
		if(root == null)
			return ;
		System.out.print(root.val + " ");
		preorderTraversal(root.left);
		preorderTraversal(root.left);
	}
}
