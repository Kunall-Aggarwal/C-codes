class LinkedList{
    Node head;
    public void insert(int data){
        Node node = new Node(data);
        
        if(head==null){
            head=node;
        }
        else{
            Node n=head;
            while(n.next!=null){
                n=n.next;
            }
            n.next=node;
        }
    }
    public void show(){
        Node node=head;
        while(node.next!=null){
            System.out.println(node.data);
            node=node.next;
        }
        System.out.print(node.data);
    }
    static class Node{
        int data;
        Node next;
        
        public Node(int data){
            this.data=data;
            next=null;
        }
    }
}
public class linklist {
    public static void main(String args[]) {
        
        LinkedList l= new LinkedList();
        l.insert(18);
        l.insert(120);
        l.insert(40);
        l.insert(6);
        
        l.show();
    }
}
