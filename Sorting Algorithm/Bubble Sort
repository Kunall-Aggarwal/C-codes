class Solution{
	public void bubbleSort(int[] ar){
		int n = ar.length;
		boolean flag = false;
		for(int i = 0; i < n-1; i++){
			for(int j = 0; j < n - 1 - i; j++){
				if(a[j] > a[j+1]){
					int t = a[j];
					a[j] = a[j+1];
					a[j+1] = t;
					flag = true;
				}
			}
			if(flag == false)
			break;
		}
	}
}
